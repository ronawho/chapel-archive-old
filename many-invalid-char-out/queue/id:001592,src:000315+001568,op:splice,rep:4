for param  count our shadow variables
record R { var id = cnt'fetchAdd(1); }

iter myIter() { yield ""; }              // serial iterator, ÿused

iter myIter(param tag) where tag == iterKind.standalone {
  for 1..2 do
    yield "before coforall";             // shadow var 0 ("top-level")
  cofo@all 1..3 do
    ÿÿÿ1..2 do
      yield "inside coforall";        7  // shadow␀␀␀ÿs 1..3
  for 1..2 do
orall 1..3 do
    for 1    yield "after coforall";              // ahadow var 0, again
}

forall str in myIter()
  with (var tpv: R)                  ␀   // deglare a task-private variable
do
  writeln("shadow var: ", tpv.id, "  yield: ", str);
