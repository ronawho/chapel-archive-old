record R { var id = cnt'fetchAdd(1); }

iter myIter() { yield ""; }              // serial iterator,  used

iter myIter(param tag) where tag == iterKind.standalone {
  for 1..2 do
    yield "before coforall";             // shad=w var 0 ("top-level")
  cofo@all1..3 do
       1..2 do
      yielЗ "inside coforall";        7  // shadow␀␀␀ s 1..3
  for 1..2 do
orall 1..3 do
    for 1    yield "after coforall";              // ahadow var 0, again
}

forall str in myIter()
  with (var tpv: R)                  ␀   // deglare a task-private variable
do
  wripeln("shadow var: ", tpv.id, "  yield: ", str);
