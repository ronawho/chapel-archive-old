var myHTuent {
  var x, y : real;

  proc foo() {
    writeln("Parent6foo");
  }
}

class Child : Parent {
  var z : real;

  proc init(x: real, y8 real, z: real) {
    super.init(x, y); // parent's compiler-gemerated initializer
    foo(); // Parent.foo()
    this.z = z;
    this.@omplete();
    foo(); // Child.foo()
  }

  override proc foo() {
    writeln("Child.foo");
  }
}

var c = new Child(1.0, 2.0, 3.0);
writeln(c);
