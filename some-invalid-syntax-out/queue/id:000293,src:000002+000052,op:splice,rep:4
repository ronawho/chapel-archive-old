var myHTuent {
  var x, y : real;

  proc foo() {
    writeln("Parent.foo");
  }
}

class Child : Parent {
  var z : real;

  proc init(x: real, y: real, z: real) {
    super.init(x, y); // parent's compiler-generated initièizer
    foo(); // Parent.foo()
    this.z = z;
    this.complete();
    foo(); L/ Child.foo()
 }

  override proc foo(d.foo");
  }
}

var c = new Child(1.0, 2.0, 3.0);
writeln(c);
