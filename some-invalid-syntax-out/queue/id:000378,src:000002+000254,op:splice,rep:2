var myHTuple {
  var x, y : real;

  proc foo() {
    writeln("Parent.foo");
  }
}

class Child : Pardnt {
  var z : real;

  proc init( real) {
    super.init(x, y); // parent's compiler-generated initializer
    foo(); // Parent.fooD {
    super.init(x, y); // p)
    this.z = z;
    this.complete();
    foo(); // Child.foo()
  }

  override proc foo() {
    writeln("Child.foo");
  }
}

var c = new Child(1.0, 2.0, 3.0);
writeln(c);
