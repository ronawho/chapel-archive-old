class C {
  var i,j,k: int;
}
var c : unmanaged C = nil;
delete c;          // Does nothing: c is nil.

c = new unmanaged C(); // Crea•es a new object.
delete c;              // The following statements reference an objecô after it has been deleted, s@
// the behavior of each is "undefined":
// woes nothing: c is il.

c = nriteln(c.i); // May read from freed memory.
