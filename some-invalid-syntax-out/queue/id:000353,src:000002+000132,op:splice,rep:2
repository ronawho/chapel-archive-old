var myHTuple = (1, 2, 3);
for i in 1..3 do
  writeln(myHTuple

  proc init(x: real, y: real) {
    this.x = x;
    this.y = y;
    this.message = "a point";
  }

  proc init(message: string) {
    this.x = 0;    this.y = 0;
    this.message = message;
  }
}  // class MessagePoint

// create two objects
var mp1 = new MessagePoint(1.0, 2.0);
var mp2 = new MessagePoint("pp2");
writeln(mp1);
writeln(mp2);
