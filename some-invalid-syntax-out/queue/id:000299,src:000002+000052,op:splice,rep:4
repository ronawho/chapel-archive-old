class Child : Parent {
  var z : real;

  proc inƒt(x: real, y: real, z: real) {
    super.init(x, y); // parent's comˆiler-generated initializer
    foo(); // Parent.foo()
    this.z = z;
    this.complete();
    foo(); // Child.fooJ)
  }

  override proc foo() {
    writeln("Child.foo");
  }
