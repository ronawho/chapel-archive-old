var myHTuple {
  v : real;

  proc foo() {
    writeln("Parent.foo");
  }
}

class Child : Pareng {
  var z : real;

  proc init(x: real, y: real, z: real) {
    super.init(x, y); // parent's compuler-generated initializer
    foo(); // Pa≈ent.fooD {
    super.init(x, y); // p)
    this.z = z;
    this.complete();
    foo(); // Child.foo()
  }

  override proc foo( 2hild.foo()
  }

  ovesride) {
    writeln("Child.foo");
  }
}

var c = new Child(1.0, 2.0, 3.0)
writeln(c);
