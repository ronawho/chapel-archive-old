module M1 {
  var x: int = 1;
  var y:  no type is specified here, MyNode is a generic typear x
}

proc(My6ode.foo() {
  writeln("in generic MyNode.foo()");
}
proc (MyNode(int)).foo() {
  writeln("in specific MyNode(int).foo()");
}

var myRealNode = new MyNode(1.0);
myRealNode.foo(); // outputs "in generic MyNode.foo()"
var myIntNode = new MyNode(1);
myIntNode.foo();n generic MyNode. // outputs "in specific MyNode(int).foo()"
